{
  "root": true,
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-recommended"
  ],
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "parser": "@babel/eslint-parser",
    "requireConfigFile": false,
    "babelOptions": {
      "presets": ["@babel/preset-env"]
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "es2021": true
  },
  "rules": {
    "vue/multi-word-component-names": "off",
    "no-console": "off",
    "no-debugger": "off"
  }
}
