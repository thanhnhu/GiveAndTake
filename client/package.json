{
  "name": "django-vue",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^20.14.0"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "vite lint",
    "test:e2e": "vite test:e2e",
    "test:unit": "vite test:unit",
    "test:unit:debug": "export VUE_CLI_BABEL_TARGET_NODE=true && export VUE_CLI_BABEL_TRANSPILE_MODULES=true && node --inspect-brk ./node_modules/jest/bin/jest.js --no-cache --runInBand"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@vee-validate/rules": "^4.15.0",
    "axios": "^1.7.9",
    "bootstrap": "^5.3.3",
    "bootstrap-vue-next": "^0.26.22",
    "core-js": "^3.40.0",
    "js-cookie": "^3.0.5",
    "moment": "^2.30.1",
    "pinia": "^2.3.1",
    "pinia-plugin-persistedstate": "^4.2.0",
    "sass": "^1.83.4",
    "sass-loader": "^16.0.4",
    "vee-validate": "^4.15.0",
    "vue": "^3.5.13",
    "vue-i18n": "^11.0.1",
    "vue-router": "^4.5.0",
    "vue-toastification": "^2.0.0-rc.5",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.7",
    "@babel/eslint-parser": "^7.26.5",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.7",
    "@babel/runtime": "^7.26.7",
    "@babel/runtime-corejs3": "^7.26.7",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/compiler-sfc": "^3.5.13",
    "@vue/eslint-config-standard": "^9.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/vue3-jest": "^29.2.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "eslint": "^9.19.0",
    "eslint-plugin-vue": "^9.32.0",
    "jest": "^29.7.0",
    "jest-date-mock": "^1.0.10",
    "unplugin-vue-components": "^28.0.0",
    "vite": "^6.0.11",
    "vite-plugin-babel": "^1.3.0",
    "vite-plugin-eslint": "^1.8.1"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false,
          "targets": "defaults"
        }
      ]
    ],
    "plugins": [
      [
        "@babel/plugin-transform-runtime",
        {
          "corejs": 3,
          "helpers": true,
          "regenerator": true,
          "useESModules": true
        }
      ]
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/eslint-config-standard"
    ],
    "rules": {
      "no-console": "off",
      "no-debugger": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
