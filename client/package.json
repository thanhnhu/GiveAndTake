{
  "name": "django-vue",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^16.14.0"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:debug": "export VUE_CLI_BABEL_TARGET_NODE=true && export VUE_CLI_BABEL_TRANSPILE_MODULES=true && node --inspect-brk ./node_modules/jest/bin/jest.js --no-cache --runInBand"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/vue-fontawesome": "^2.0.2",
    "@vue/cli-service-global": "^4.5.13",
    "axios": "^0.21.1",
    "bootstrap": "4.5.3",
    "bootstrap-vue": "2.21.2",
    "js-cookie": "^3.0.0",
    "moment": "^2.29.1",
    "sass": "^1.22.10",
    "sass-loader": "10.1.1",
    "vee-validate": "^3.4.11",
    "vue": "^2.6.14",
    "vue-i18n": "^8.25.0",
    "vue-router": "^3.5.2",
    "vuex": "^3.6.2",
    "vuex-persistedstate": "^4.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-plugin-unit-jest": "^3.2.0",
    "@vue/cli-service": "^4.5.13",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "^7.0.0-bridge.0",
    "flush-promises": "^1.0.2",
    "jest-date-mock": "^1.0.6",
    "vue-template-compiler": "^2.6.14"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-debugger": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
